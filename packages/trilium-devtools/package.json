{
  "name": "trilium-devtools",
  "version": "0.1.0",
  "author": "Patrick Moriarty @patrickjm",
  "bin": {
    "trilium-devtools": "./bin/run"
  },
  "bugs": "https://github.com/patrickjm/trilium-devtools/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/ini": "^1.3.30",
    "@types/node-fetch": "^2.5.9",
    "@types/shelljs": "^0.8.8",
    "cross-env": "^7.0.3",
    "ini": "^2.0.0",
    "node-fetch": "^2.6.1",
    "shelljs": "^0.8.4",
    "tree-kill": "^1.2.2",
    "tslib": "^1",
    "webpack": "^5.28.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/webpack": "^5.28.0",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/patrickjm/trilium-devtools",
  "license": "APGL-3.0-only",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "trilium-devtools",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "patrickjm/trilium-devtools",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
